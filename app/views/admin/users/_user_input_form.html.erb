<%= form_for @user do |f| %>
    <p>
      <%= f.label "name" %><br>
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.label "email" %><br>
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label "phone number" %><br>
      <%= f.text_field :phone_number %>
    </p>
    <p>
      <%= f.label "type" %><br>
      <%= f.text_field :type %>
    </p>
    <p>
      <%= submit_tag "Save" %>
    </p>
<% end %>