<%= form_for @restaurant, url: edit_admin_restaurant_path do |f| %>

    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= f.label :capacity %>
      <%= f.text_field :capacity %>
    </div>
    <div>
      <%= f.label :phone_number %>
      <%= f.text_field :phone_number %>
    </div>
    <div>
      <%= f.label :street_number%>
      <%= f.text_field :street_number %>
    </div>
    <div>
      <%= f.label :street_name %>
      <%= f.text_field :street_name %>
    </div>
    <div>
      <%= f.label :city %>
      <%= f.text_field :city %>
    </div>
    <div>
      <%= f.label :province %>
      <%= f.text_field :province %>
    </div>
    <div>
      <%= f.label :country %>
      <%= f.text_field :country %>
    </div>
    <div>
      <%= f.label :postal_code %>
      <%= f.text_field :postal_code %>
    </div>
    <div>
      <%= f.submit "Save restaurant" %>
    </div>
<% end %>