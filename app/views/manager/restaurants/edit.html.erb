<%= form_for [:manager,@restaurant] do |f| %>
    <div>Restaurant Name <%= f.text_field(:name) %></div>
    <div>Restaurant Phone Number <%= f.phone_field(:phone_number) %></div>
    <div>Capacity<%= f.phone_field(:capacity) %></div>
    <div>street_number<%= f.text_field(:street_number) %></div>
    <div>street_name<%= f.text_field(:street_name) %></div>
    <div>city<%= f.text_field(:city) %></div>
    <div>province<%= f.text_field(:province) %></div>
    <div>country<%= f.text_field(:country) %></div>
    <div>postal_code<%= f.text_field(:postal_code) %></div>
    <%= f.submit %>
<% end %>